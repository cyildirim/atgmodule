<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:App/etc/routes.xsd">
    <!--Use router 'admin' for admin route -->
    <router id="admin">
        <!--Define a custom route with id and frontName -->
        <route id="mycompany_exampleadminnewpage" frontName="mycompany_exampleadminnewpage">
            <!--The module which this route match to-->
            <module name="MyCompany_ExampleAdminNewPage"/>
        </route>

        <route id="mycompany_exampleadminnewpage_cuscatalog" frontName="mycompany_exampleadminnewpage_cuscatalog_front">
            <!--The module which this route match to-->
            <module name="MyCompany_ExampleAdminNewPage"/>
        </route>

        <route id="mycompany_exampleadminnewpage_newform" frontName="mycompany_exampleadminnewpage_newform_front">
            <!--The module which this route match to-->
            <module name="MyCompany_ExampleAdminNewPage"/>
        </route>
    </router>
</config>